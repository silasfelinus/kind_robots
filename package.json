{
  "name": "kind-robots",
  "private": true,
  "scripts": {
    "dev": "nuxt dev --host",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lint:js": "eslint .",
    "lint:prettier": "prettier --check .",
    "prettier": "prettier --write .",
    "lintfix": "eslint --fix . && prettier --write .",
    "studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate && npm run prisma:inline",
    "prisma:migrate": "npx prisma migrate deploy && npm run prisma:inline",
    "prisma:migrate:dev": "npx prisma migrate dev && npm run prisma:inline",
    "prisma:inline": "cp ./node_modules/.prisma/client/*.js ./node_modules/@prisma/client",
    "postinstall": "npm run prisma:generate && nuxt prepare"
  },
  "devDependencies": {
    "@nuxt/content": "^2.9.0",
    "@nuxt/eslint": "^0.5.0",
    "@nuxtjs/tailwindcss": "^6.7.0",
    "@prisma/nextjs-monorepo-workaround-plugin": "^5.6.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/bcrypt": "^5.0.0",
    "@types/canvas-confetti": "^1.6.0",
    "@types/node": "^20.8.10",
    "@unlok-co/nuxt-stripe": "^2.0.0",
    "bcrypt": "^5.1.0",
    "daisyui": "^3.9.4",
    "eslint": "^8.38.0",
    "nuxt": "latest",
    "nuxt-icon": "^0.6.1",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "prisma": "^5.15.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@interactjs/interact": "^1.10.17",
    "@interactjs/types": "^1.10.18",
    "@nuxt/image": "^1.0.0",
    "@nuxtjs/axios": "^5.13.6",
    "@pinia/nuxt": "^0.5.1",
    "@prisma/client": "^5.14.0",
    "@stripe/stripe-js": "^2.1.7",
    "canvas-confetti": "^1.6.0",
    "gsap": "^3.12.2",
    "interactjs": "^1.10.18",
    "jose": "^5.1.0",
    "open-simplex-noise": "^2.5.0",
    "pinia": "^2.1.6",
    "stripe": "^14.3.0",
    "theme-change": "^2.5.0"
  },
  "overrides": {
    "vue": "latest"
  }
}
